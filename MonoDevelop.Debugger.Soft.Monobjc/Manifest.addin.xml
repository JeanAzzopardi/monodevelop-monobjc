<Addin id = "MonoDevelop.Debugger.Soft.Monobjc"
       name	     = "Mono Soft Debugger Support for Monobjc"
       author	 = "Eric Butler"
       url       = "http://codebutler.github.com/monodevelop-monobjc/"
       description = "Mono Soft Debugger Support for Monobjc"
       copyright   = "MIT X11"
       category = "Debugging"
       version   = "0.5">

	<Runtime>
		<Import assembly="MonoDevelop.Debugger.Soft.Monobjc.dll" />
	</Runtime>

	<Dependencies>
		<Addin id="MonoDevelop.Core" version="2.2"/>
		<Addin id="MonoDevelop.Debugger" version="2.2"/>
		<Addin id="MonoDevelop.Debugger.Soft" version="2.2"/>
		<Addin id="MonoDevelop.Monobjc" version="0.5"/>
	</Dependencies>
	
	<Extension path="/MonoDevelop/Debugging/DebuggerFactories">
		<DebuggerFactory type="MonoDevelop.Debugger.Soft.Monobjc.MonobjcSoftDebuggerEngine" />
	</Extension>
</Addin>
